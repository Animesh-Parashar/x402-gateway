{
  "name": "@x402/adapter-mock",
  "version": "1.0.0",
  "description": "Mock facilitator adapter for x402 payment gateway testing and development",
  "keywords": [
    "x402",
    "mock",
    "adapter",
    "payment",
    "facilitator",
    "testing",
    "402"
  ],
  "author": "x402 Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402-org/x402-gateway.git",
    "directory": "packages/adapter-mock"
  },
  "homepage": "https://github.com/x402-org/x402-gateway#readme",
  "bugs": {
    "url": "https://github.com/x402-org/x402-gateway/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@x402/core": "1.0.0",
    "undici": "^7.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
