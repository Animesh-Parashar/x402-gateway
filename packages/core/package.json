{
  "name": "@x402/core",
  "version": "1.0.0",
  "description": "Core types, errors, and utilities for x402 HTTP payment gateway middleware",
  "keywords": [
    "x402",
    "payment",
    "402",
    "gateway",
    "middleware",
    "http",
    "crypto",
    "blockchain"
  ],
  "author": "x402 Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402-org/x402-gateway.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/x402-org/x402-gateway#readme",
  "bugs": {
    "url": "https://github.com/x402-org/x402-gateway/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}