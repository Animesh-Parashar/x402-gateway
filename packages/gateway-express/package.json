{
  "name": "@x402/gateway-express",
  "version": "1.0.0",
  "description": "Express.js middleware for x402 HTTP payment gateway - require cryptographic payments per request",
  "keywords": [
    "x402",
    "express",
    "middleware",
    "payment",
    "402",
    "gateway",
    "http",
    "crypto"
  ],
  "author": "x402 Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/x402-org/x402-gateway.git",
    "directory": "packages/gateway-express"
  },
  "homepage": "https://github.com/x402-org/x402-gateway#readme",
  "bugs": {
    "url": "https://github.com/x402-org/x402-gateway/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@x402/core": "1.0.0"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
